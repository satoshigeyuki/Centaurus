grammar JsonEBNF;

Object : Number | String | List | Dictionary | True | False | Null;
Number : /-?[0-9]++[.]?[0-9]*+/ ;
//String : /"([\t\n\r !#-\[\]-~]|(\\["]))*"/ ;
String : /"([^\\"]|(\\["]))*+"/ ;
List : '[' Object? (',' Object)* ']' ;
DictionaryEntry : String ':' Object ;
Dictionary : '{' DictionaryEntry? (',' DictionaryEntry)* '}' ;
True : 'true' ;
False : 'false' ;
Null : 'null' ;
